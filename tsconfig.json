{
    "exclude": [
        "node_modules",
        "js"
    ],
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./js",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
